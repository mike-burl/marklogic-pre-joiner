<properties>
	<connection>
		<hosts>
			<host>localhost</host>
		</hosts>
		<port>8011</port>
		<numThreadsPerHost>100</numThreadsPerHost>
		<batchSize>1</batchSize>
		<username>CHANGEME</username>
		<password>CHANGEME</password>
	</connection>
	<data>
		<entity>test</entity>
		<namespace>http://www.test.com/namespace</namespace>
		<uriPrefix>/testData/PRIMARY/</uriPrefix>
		<urlSuffix>.xml</urlSuffix>
		<primaryCSV>
			<location>data/PRIMARY.csv</location>
			<header>PRIMARY_ID,PRIMARY_CHAR,FNAME,LNAME,SSN</header>
			<primaryKey>PRIMARY_ID</primaryKey>
			<name>PRIMARY</name>
			<separator>,</separator>
		</primaryCSV>
		<childCSVs>
			<childCSV>
				<location>data/CLAIM.csv</location>
				<header>PRIMARY_ID,PRIMARY_CHAR,VAL1,VAL2</header>
				<primaryKey>PRIMARY_ID</primaryKey>
				<name>CLAIM</name>
				<separator>,</separator>
			</childCSV>
			<childCSV>
				<location>data/ADDRESS.csv</location>
				<header>PRIMARY_ID,PRIMARY_CHAR,ADDRESS_TYPE,ADDRESS,CITY,STATE,ZIP</header>
				<primaryKey>PRIMARY_ID</primaryKey>
				<name>ADDRESS</name>
				<separator>,</separator>
			</childCSV>
		</childCSVs>
	</data>
</properties>