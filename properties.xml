<properties>
	<connection>
		<hosts>
			<host>localhost</host>
		</hosts>
		<port>8011</port>
		<numThreadsPerHost>10</numThreadsPerHost>
		<batchSize>2</batchSize>
		<username>CHANGEME</username>
		<password>CHANGEME</password>
		<authContext>digest</authContext> <!--digest or basic, default: digest-->
	</connection>
	<data>
		<parser>STANDARD_PREFERENCE</parser>
		<entity>
			<entityName>test</entityName>
			<namespace>http://www.test.com/namespace</namespace>
			<uriPrefix>/testData/PRIMARY/</uriPrefix>
			<uriSuffix>.xml</uriSuffix>
			<primaryKey>PRIMARY_ID</primaryKey>
			<name>PRIMARY</name>
			<collections>
				<collection>mlps-test-1</collection>
				<collection>mlps-test-2</collection>
			</collections>
		</entity>
		<primaryCSV>
			<location>data/PRIMARY.csv</location>
			<header>PRIMARY_ID,PRIMARY_CHAR,FNAME,LNAME,SSN</header>
			<primaryKey>PRIMARY_ID</primaryKey>
			<name>PRIMARY</name>
			<separator>,</separator>
		</primaryCSV>
		<childCSVs>
			<childCSV>
				<location>data/CLAIM.csv</location>
				<header>PRIMARY_ID,PRIMARY_CHAR,VAL1,VAL2</header>
				<primaryKey>PRIMARY_ID</primaryKey>
				<name>CLAIM</name>
				<separator>,</separator>
			</childCSV>
			<childCSV>
				<location>data/ADDRESS.csv</location>
				<header>PRIMARY_ID,PRIMARY_CHAR,ADDRESS_TYPE,ADDRESS,CITY,STATE,ZIP</header>
				<primaryKey>PRIMARY_ID</primaryKey>
				<name>ADDRESS</name>
				<separator>,</separator>
			</childCSV>
		</childCSVs>
	</data>
</properties>